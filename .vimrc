syntax on

"colorscheme desert256
"colorscheme jeyllybeans
"colorscheme midnight
"colorscheme sierra
"colorscheme twilight
"colorscheme wombat
colorscheme wwdc16
:set t_Co=256

highlight Normal ctermfg=250

set number

:autocmd FileType c,cpp  set formatoptions=croql

:ab #i #include
:ab #b /********************************************************
:ab #e ********************************************************/
:ab #l /*------------------------------------------------------*/

:set tabstop=4
:set shiftwidth=4
:set expandtab
:set autoindent
:set cindent
:set hlsearch
:set incsearch
:set textwidth=120
